{
    "name": "mindvista",
    "version": "2.0.0",
    "description": "The Home Base of mindvista.ca: Where It All Comes Together",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
        "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
        "setup:dev": "tsx --env-file=.env scripts/environments/setup-local-dev.ts",
        "sync:db:dev": "tsx --env-file=.env scripts/environments/development/seed-db.ts",
        "sync:db:staging": "tsx --env-file=.env.staging scripts/environments/staging/seed-db.ts",
        "sync:storage:dev": "tsx --env-file=.env scripts/environments/development/seed-s3.ts",
        "sync:storage:staging": "tsx --env-file=.env.staging scripts/environments/staging/seed-s3.ts",
        "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
        "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
        "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
        "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
        "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
        "commitlint": "commitlint --edit",
        "format": "prettier --write .",
        "prepare": "husky install",
        "test": "vitest watch"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.758.0",
        "@hcaptcha/react-hcaptcha": "^1.11.2",
        "@hookform/resolvers": "^3.9.1",
        "@mailchimp/mailchimp_marketing": "^3.0.80",
        "@payloadcms/db-postgres": "^3.25.0",
        "@payloadcms/db-vercel-postgres": "^3.25.0",
        "@payloadcms/email-resend": "^3.25.0",
        "@payloadcms/live-preview-react": "^3.25.0",
        "@payloadcms/next": "^3.25.0",
        "@payloadcms/payload-cloud": "^3.25.0",
        "@payloadcms/richtext-lexical": "^3.25.0",
        "@payloadcms/storage-s3": "^3.25.0",
        "@vercel/analytics": "^1.4.1",
        "@vercel/functions": "^2.0.0",
        "@vercel/speed-insights": "^1.1.0",
        "@web3forms/react": "^1.1.3",
        "canvas-confetti": "^1.9.3",
        "cross-env": "^7.0.3",
        "date-fns-tz": "^3.2.0",
        "graphql": "^16.8.1",
        "next": "15.2.0",
        "nextjs-toploader": "^3.7.15",
        "payload": "^3.25.0",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-hook-form": "^7.54.2",
        "react-i18next": "^15.4.0",
        "react-icons": "^5.4.0",
        "sharp": "0.32.6",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.6.1",
        "@commitlint/config-conventional": "^19.6.0",
        "@eslint/eslintrc": "^3.2.0",
        "@playwright/test": "^1.51.0",
        "@tailwindcss/typography": "^0.5.15",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.2.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/canvas-confetti": "^1.9.0",
        "@types/mailchimp__mailchimp_marketing": "^3.0.21",
        "@types/node": "^22.5.4",
        "@types/react": "19.0.10",
        "@types/react-dom": "19.0.4",
        "@types/tar": "^6.1.13",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "conventional-changelog-atom": "^5.0.0",
        "eslint": "^9.16.0",
        "eslint-config-next": "15.2.0",
        "git-format-staged": "^3.1.1",
        "husky": "^8.0.3",
        "jsdom": "^26.0.0",
        "postcss": "^8.4.49",
        "prettier": "^3.4.2",
        "prettier-plugin-tailwindcss": "^0.6.9",
        "tailwindcss": "^3.4.17",
        "tar": "^7.4.3",
        "typescript": "5.7.2",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.0.7"
    },
    "engines": {
        "node": "^18.20.2 || >=20.9.0"
    },
    "overrides": {
        "@types/react": "19.0.10",
        "@types/react-dom": "19.0.4"
    }
}
